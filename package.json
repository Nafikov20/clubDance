{
  "name": "winkid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test:all": "yarn test:eslint && yarn test:prettier && yarn test:stylelint",
    "test:all:fix": "yarn test:eslint:fix && yarn test:prettier && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx,js}'",
    "test:eslint:fix": "eslint --fix '**/*.{ts,tsx,js}'",
    "test:stylelint": "stylelint '**/*.css'",
    "test:stylelint:fix": "stylelint --fix '**/*.css'",
    "test:prettier": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "test:commit": "commitlint -e"
  },
  "dependencies": {
    "@sentry/react": "7.10.0",
    "@tanstack/react-query": "4.1.3",
    "@tanstack/react-query-devtools": "4.0.10",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "dayjs": "1.11.5",
    "effector": "22.3.0",
    "effector-react": "22.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.1",
    "react-hot-toast": "2.3.0",
    "react-loading-skeleton": "3.1.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "rooks": "6.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/node": "18.7.3",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "autoprefixer": "10.4.8",
    "cssnano": "5.1.13",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "stylelint": "14.10.0",
    "stylelint-config-standard": "27.0.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

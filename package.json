{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test:all": "yarn tsc --noEmit && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn tsc --noEmit && yarn test:eslint:fix && yarn test:prettier && yarn test:stylelint:fix",
    "test:eslint": "eslint './src/**/*.{ts,tsx}'",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint './src/**/*.css'",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier": "prettier --write './src/**/*.{ts,tsx,json,md}'",
    "test:commit": "commitlint -e"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/react": "7.27.0",
    "@tanstack/react-query": "4.20.4",
    "@tanstack/react-query-devtools": "4.20.4",
    "axios": "1.2.1",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "effector": "22.4.1",
    "effector-react": "22.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.0",
    "react-hot-toast": "2.4.0",
    "react-loading-skeleton": "3.1.0",
    "react-router-dom": "6.5.0",
    "react-scripts": "5.0.1",
    "rooks": "7.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "autoprefixer": "10.4.13",
    "cssnano": "5.1.14",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-effector": "0.10.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "stylelint": "14.16.0",
    "stylelint-config-clean-order": "2.1.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
